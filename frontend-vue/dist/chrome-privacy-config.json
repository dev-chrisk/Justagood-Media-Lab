{
  "name": "Media Library - Privacy Configuration",
  "version": "1.0.0",
  "description": "Privacy-focused configuration for Media Library application",
  "privacy_settings": {
    "third_party_cookies": {
      "enabled": false,
      "reason": "Application uses backend proxy for all external API calls"
    },
    "cross_site_tracking": {
      "enabled": false,
      "reason": "No cross-site tracking implemented"
    },
    "data_collection": {
      "minimal": true,
      "reason": "Only essential data for application functionality"
    }
  },
  "cors_policy": {
    "allowed_origins": [
      "http://localhost:3000",
      "http://127.0.0.1:3000",
      "http://localhost:5173",
      "http://127.0.0.1:5173"
    ],
    "allowed_methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "allowed_headers": ["Content-Type", "Authorization", "X-Requested-With"]
  },
  "cookie_policy": {
    "same_site": "Lax",
    "secure": false,
    "http_only": true,
    "max_age": 3600
  }
}


